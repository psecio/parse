{
    "name":"psecio/parse",
    "type":"library",
    "description":"A PHP Security Scanner",
    "keywords":["security","scanner","static"],
    "homepage":"https://github.com/psecio/parse.git",
    "license":"MIT",
    "authors":[
        {
            "name":"Chris Cornutt",
            "email":"ccornutt@phpdeveloper.org",
            "homepage":"http://www.phpdeveloper.org/"
        }
    ],
    "require":{
        "php": ">=5.4",
        "nikic/php-parser": "^2.0",
        "symfony/console": "2.5 - 3.2",
        "symfony/event-dispatcher": "2.4 - 3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.2",
        "codeclimate/php-test-reporter": "dev-master",
        "mockery/mockery": "^0.9",
        "akamon/mockery-callable-mock": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Psecio\\Parse\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Psecio\\Parse\\": "tests/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "bin": [
        "bin/psecio-parse",
        "bin/parse"
    ]
}
